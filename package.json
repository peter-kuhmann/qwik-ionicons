{
  "name": "@tectonique/ionicons-qwik",
  "version": "1.0.0",
  "description": "Ionicons for Qwik ⚡️",
  "repository": "https://github.com/tectonique/ionicons-qwik",
  "license": "MIT",
  "author": {
    "name": "Peter Kuhmann",
    "email": "peter@hedgehogs-mind.com"
  },
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "release": "np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "clean": "rimraf lib && rimraf lib-types && cd .generator && yarn clean && cd ..",
    "runGenerator": "cd .generator && yarn generate && cd ..",
    "prepublishOnly": "yarn clean && yarn runGenerator && yarn build.lib && yarn build.types"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.103.0",
    "@types/eslint": "8.37.0",
    "@types/node": "^18.15.9",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "latest",
    "np": "7.6.1",
    "prettier": "2.8.7",
    "rimraf": "^5.0.0",
    "typescript": "5.0.4",
    "undici": "5.21.2",
    "vite": "4.2.2"
  }
}
